{
  "name": "resume",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npx playwright install && npx playwright install-deps",
    "reinstall": "rm -rf node_modules && rm -rf .next && rm package-lock.json && npm i",
    "update:ts": "npm i -D typescript@latest @types/node@latest @types/react@latest @types/react-dom@latest",
    "update:next": "npm i next@latest react@latest react-dom@latest",
    "update:tailwind": "npm i -D tailwindcss@latest @tailwindcss/postcss@latest",
    "update:icons": "npm i react-icons@latest",
    "update:playwright": "npm i playwright-chromium@latest",
    "update:eslint": "npm i -D eslint@latest eslint-config-next@latest",
    "update:biome": "npm i -D @biomejs/biome@latest",
    "update:all-dependencies": "npm run update:ts && npm run update:next && npm run update:tailwind && npm run update:icons && npm run update:playwright && npm run update:eslint && npm run update:biome && npm ls && npm outdated"
  },
  "dependencies": {
    "next": "^15.4.5",
    "playwright-chromium": "^1.54.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  }
}